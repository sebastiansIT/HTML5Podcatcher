{
  "name": "HTML5Podcatcher",
  "version": "0.51.2",
  "description": "A HTML5 based podcast player.",
  "license": "GPL-3.0",
  "main": "Gruntfile.js",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "grunt": "^1.1.0",
    "grunt-concat-css": "^0.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^2.0.0",
    "grunt-contrib-jasmine": "^2.2.0",
    "grunt-curl": "^2.5.1",
    "grunt-html": "^14.0.0",
    "grunt-jslint": "^1.1.15",
    "grunt-postcss": "^0.9.0",
    "grunt-string-replace": "^1.3.1",
    "grunt-usemin": "^3.1.1",
    "grunt-zip": "^0.18.2",
    "jsdoc": "^3.6.4",
    "karma": "^5.0.4",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-rollup-preprocessor": "^7.0.5",
    "rollup": "^2.8.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansIT/HTML5Podcatcher.git"
  },
  "author": "SebastiansIT",
  "bugs": {
    "url": "https://github.com/sebastiansIT/HTML5Podcatcher/issues"
  },
  "homepage": "http://podcatcher.sebastiansit.de",
  "scripts": {
    "lint-js": "eslint -c .eslintrc.js sources/webapp/scripts/api/commands/**/*.js",
    "test-js": "karma start karma.config.js",
    "jsdoc-api": "jsdoc -r -a all -P package.json -d distributions/jsdoc/api sources/webapp/scripts/api",
    "jsdoc-ui": "jsdoc -r -a all -d distributions/jsdoc/ui sources/webapp/scripts/ui",
    "doc": "npm run jsdoc-api && npm run jsdoc-ui",
    "bundle-js": "rollup -c",
    "lint": "npm run lint-js",
    "test": "npm run test-js",
    "build": "grunt HostedWebApp && npm run bundle-js",
    "watch": "karma start karma.config.js --no-single-run"
  }
}
